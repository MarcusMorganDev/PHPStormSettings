<templateSet group="user">
  <template name="ajax" value="$j.ajax({&#10;          type: '$type$',&#10;          url: '$url$',&#10;          dataType: '$dataType$',&#10;          data: $data$&#10;      })" description="jquery ajax call" toReformat="true" toShortenFQNames="true">
    <variable name="type" expression="" defaultValue="POST" alwaysStopAt="true" />
    <variable name="url" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="dataType" expression="" defaultValue="json" alwaysStopAt="true" />
    <variable name="data" expression="" defaultValue="" alwaysStopAt="true" />
